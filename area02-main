-- Scripts:

local function OBXDHS_fake_script() -- UIListLayout.AutoSize 
	local script = Instance.new('LocalScript', UIListLayout)

	script.Parent:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		local absoluteSize = script.Parent.AbsoluteContentSize
		script.Parent.Parent.CanvasSize = UDim2.new(0,absoluteSize.X, 0, absoluteSize.Y)
	end)
end
coroutine.wrap(OBXDHS_fake_script)()
local function PZEFE_fake_script() -- Output.Hide 
	local script = Instance.new('LocalScript', Output)

	script.Parent.Visible = false
end
coroutine.wrap(PZEFE_fake_script)()
local function NFAU_fake_script() -- Input.Core 
	local script = Instance.new('LocalScript', Input)

	local playerdiscordid = "TestID"
	local InputBox = script.Parent
	local ContextActionService = game:GetService("ContextActionService")
	local ACTION_NAME = "FocusTheTextBox"
	local textBox = script.Parent
	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()
	previouscommand1 = nil -- Guarda el comando anterior para luego volverlo a poner con la tecla de flecha up.
	previouscommand2 = nil -- Mismo
	previouscommand3 = nil -- Mismo
	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()
	local UIS = game:GetService("UserInputService")
	
	UIS.InputBegan:Connect(function(input, gameProccessedEvent) -- Sistema para recuperar comandos anteriores.
		if input.UserInputType == Enum.UserInputType.Keyboard then
			if input.KeyCode == Enum.KeyCode.Up then
				if previouscommand1 then
					if script.Parent.Text == previouscommand1 then
						if previouscommand2 then
							if script.Parent.Text == previouscommand2 then
								if previouscommand3 then
									if script.Parent.Text ~= previouscommand3 then
										script.Parent.Text = previouscommand3
									end
								end
							else
								script.Parent.Text = previouscommand2
							end
						end
					else
						script.Parent.Text = previouscommand1
					end
				end
			end
		end
	end)
	
	
	local function handleAction(actionName, inputState, inputObject)
		if actionName == ACTION_NAME and inputState == Enum.UserInputState.Begin then
			wait()
			textBox:CaptureFocus()
		end
	end
	ContextActionService:BindAction(ACTION_NAME, handleAction, false, Enum.KeyCode.Period)
	-- Cuando la cantidad de texto de la textbox supera los 35 caracteres, se hace una animación aquí.
	script.Parent:GetPropertyChangedSignal("Text"):Connect(function()
		local pure  = string.gsub(script.Parent.Text, "[%.!?]", "")
		script.Parent.Text = pure
		if string.sub(script.Parent.Text,1,25) then
			script.Parent.Text = string.sub(script.Parent.Text,1,35)
		end
		if #script.parent.Text == 35 then
			if not alrdoing then
				alrdoing = true
				local ogcolor = script.Parent.TextColor3
				wait()
				script.Parent.TextColor3 = Color3.new(1, 0, 0)
				wait(0.5)
				script.Parent.TextColor3 = ogcolor
				alrdoing = false
			end
		end
	end)
	-- Notificar positivamente sobre algo (no errores)
	function notifyok(info)
		local newtextbox = script.Parent.Parent.Parent.ExampleListLabel:Clone()
		newtextbox.Visible = true
		newtextbox.Text = "[+]".." "..info
		newtextbox.Parent = script.Parent.Parent.Parent.Output.ScrollingFrame
		if not alrdoing then
			alrdoing = true
			local ogcolor = script.Parent.TextColor3
			wait()
			script.Parent.TextColor3 = Color3.new(0, 1, 0)
			wait(0.5)
			script.Parent.TextColor3 = ogcolor
			script.Parent.Text = ""
			alrdoing = false
		end
	end
	-- Notificar negativamente sobre algo (error o falta de argumentos)
	function notifyerror(info)
		local newtextbox = script.Parent.Parent.Parent.ExampleListLabel:Clone()
		newtextbox.Visible = true
		newtextbox.Text = "[-]".." "..info
		newtextbox.Parent = script.Parent.Parent.Parent.Output.ScrollingFrame
		if not alrdoing then
			alrdoing = true
			local ogcolor = script.Parent.TextColor3
			wait()
			script.Parent.TextColor3 = Color3.new(1, 1, 0)
			wait(0.5)
			script.Parent.TextColor3 = ogcolor
			script.Parent.Text = ""
			alrdoing = false
		end
	end
	function unknowncommand()
		local newtextbox = script.Parent.Parent.Parent.ExampleListLabel:Clone()
		newtextbox.Visible = true
		newtextbox.Text = "[!] Unknown command."
		newtextbox.Parent = script.Parent.Parent.Parent.Output.ScrollingFrame
		if not alrdoing then
			alrdoing = true
			local ogcolor = script.Parent.TextColor3
			wait()
			script.Parent.TextColor3 = Color3.new(1, 0, 0)
			wait(0.5)
			script.Parent.TextColor3 = ogcolor
			script.Parent.Text = ""
			alrdoing = false
		end
	end
	-- El jugador pone un texto en el input y hace enter
	function team(team)
		local args = {
			[1] = team,
		}
		game:GetService("ReplicatedStorage").RemoteFunction.RequestTeam:InvokeServer(unpack(args))
		wait(3)
	end
	-- Obtiene un jugador desde una abreviación. (EJ: Koa = KoalaKMD)
	function GetPlayerFromString(nameString)
		if not nameString then return end 
		local matches= {}
		for _, player in ipairs (game.Players:GetPlayers()) do
			if string.lower(player.Name):match(string.lower(nameString)) then
				table.insert(matches, player)
			end
		end
		if #matches== 1 then
			return matches[1]
		else
			return false
		end
	end
	-- Chequea si el jugador tiene herramientas en el inventario.
	function tools(plr)
		if plr:FindFirstChildOfClass("Backpack"):FindFirstChildOfClass('Tool') or plr.Character:FindFirstChildOfClass('Tool') then
			return true
		end
	end
	-- Obtiene la HumanoidRootPart de un character.
	function getRoot(char)
		local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
		return rootPart
	end
	-- Une a un jugador con otro usando herramientas.
	function attach(target)
		local speaker = game.Players.LocalPlayer
		if tools(speaker) then
			local char = speaker.Character
			local tchar = target.Character
			local hum = speaker.Character:FindFirstChildOfClass("Humanoid")
			local hrp = getRoot(speaker.Character)
			local hrp2 = getRoot(target.Character)
			hum.Name = "1"
			local newHum = hum:Clone()
			newHum.Parent = char
			newHum.Name = "Humanoid"
			wait()
			hum:Destroy()
			workspace.CurrentCamera.CameraSubject = char
			newHum.DisplayDistanceType = "None"
			local tool = speaker:FindFirstChildOfClass("Backpack"):FindFirstChildOfClass("Tool") or speaker.Character:FindFirstChildOfClass("Tool")
			tool.Parent = char
			hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
			local n = 0
			repeat
				wait(.1)
				n = n + 1
				hrp.CFrame = hrp2.CFrame
			until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
			print("Attached.")
		else
			notifyerror("Bring failed. Try again.")
		end
	end
	-- Chequea si un jugador está vivo.
	function isAlive(player)
		if player.Character:FindFirstChild("Humanoid") then
			if player.Character:FindFirstChild("Humanoid").Health > 0 then
				return true
			else
				return false
			end
		else
			return false
		end
	end
	-- Bring pero versión para el comando terminate.
	function terminate(target)
		local speaker = game.Players.LocalPlayer
		if tools(speaker) then
			if target ~= nil then
				local NormPos = getRoot(speaker.Character).CFrame
				local NormVector = speaker.Character:FindFirstChild("HumanoidRootPart").Position
				speaker.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(-153.522995, 603.280518, 326.786438)
				wait(1)
				local hrp = getRoot(speaker.Character)
				attach(target)
				for i=0,10 do
					hrp.CFrame = NormPos
				end
				wait(1)
				speaker.CharacterAdded:Wait()
				wait(1)
				speaker.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(NormVector)
			end
		else
			notifyerror("Failed bringing: No tools.")
		end
	end
	-- Trae a un jugador hacia el localplayer.
	function bring(speaker,target)
		if tools(speaker) then
			if target ~= nil then
				local NormPos = getRoot(speaker.Character).CFrame
				local NormVector = speaker.Character:FindFirstChild("HumanoidRootPart").Position
				local hrp = getRoot(speaker.Character)
				attach(target)
				for i=0,10 do
					hrp.CFrame = NormPos
				end
				wait(1)
				speaker.CharacterAdded:Wait()
				wait(1)
				speaker.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(NormVector)
			end
		else
			notifyerror("Failed bringing: No tools.")
		end
	end
	function cikill(target, gunname, shoots)
		if game:GetService("Players").LocalPlayer.Character:FindFirstChild(gunname) then
			if gunname then
				if target then
					if target.Character:FindFirstChild("Humanoid") ~= nil then
						local args = {
							[1] = Vector3.new(1424.7781982422, 672.20568847656, -678.8935546875),
							[2] = target.Character.Humanoid,
							[3] = game:GetService("Players").LocalPlayer.Character[gunname]
						}
						for i=0, shoots do
							if target then
								game:GetService("ReplicatedStorage").WeaponReplicatedStorage.Hit:FireServer(unpack(args))
							end
						end
					end
				else
					return false
				end
			else
				return false
			end
		else
			return false
		end
	
	end
	function tweensize(uiobject, speed, finaludim)
		uiobject:TweenSize(finaludim, 'Out', 'Quint', speed)
		wait(speed + 0.1)
	end
	function getAmountOfPlayersInTeam(teamName)
		local Teams = game:GetService("Teams")
		local Team = Teams[teamName]
	
		local function getTeamSize(team)
			return #team:GetPlayers()
		end
		return getTeamSize(Team)
	end
	function FakeChar(option)
		if option == true then
			FakeCameraSubject = true
			game.Players.LocalPlayer.Character.Archivable = true
			local newchar = game.Players.LocalPlayer.Character:Clone()
			newchar.Parent = game.Workspace
			newchar.Name = "FakeChar"
			newchar["HumanoidRootPart"].CFrame = CFrame.new(game.Players.LocalPlayer.Character["HumanoidRootPart"].Position)
			workspace.CurrentCamera.CameraSubject = newchar.Humanoid
			local bambam = Instance.new("BodyThrust")
			bambam.Parent = newchar["HumanoidRootPart"]
			bambam.Force = Vector3.new(50,0,50)
			bambam.Location = newchar["HumanoidRootPart"].Position
		else
			FakeCameraSubject = false
			workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character["Humanoid"]
			game.Workspace["FakeChar"]:Destroy()
		end
	end
	function disclaimer()
		local disclaimerFrame = script.Parent.Parent.Parent.Disclaimer
		disclaimerFrame.Visible = true
		tweensize(disclaimerFrame, 1, UDim2.new(0, 477,0, 164))
		repeat
			wait()
		until disclaimerOption == "YES" or disclaimerOption == "NO"
		if disclaimerOption == "YES" then
			tweensize(disclaimerFrame, 1, UDim2.new(0, 0,0, 164))
			script.Parent.Parent.Parent.Disclaimer.Visible = false
			disclaimerOption = nil
			return true
		elseif disclaimerOption == "NO" then
			tweensize(disclaimerFrame, 1, UDim2.new(0, 0,0, 164))
			script.Parent.Parent.Parent.Disclaimer.Visible = false
			disclaimerOption = nil
			return false
		end
	end
	function spoofPlayerInfo(option) -- Esconde el cosito de información del jugador en la esquina inferior izquierda.
		if option == true then
			game:GetService("Players").LocalPlayer.PlayerGui.PlayerInfo.Main.Info.Visible = false
			game:GetService("Players").LocalPlayer.PlayerGui.PlayerInfo.Main.XP.Visible = false
		else
			game:GetService("Players").LocalPlayer.PlayerGui.PlayerInfo.Main.Info.Visible = true
			game:GetService("Players").LocalPlayer.PlayerGui.PlayerInfo.Main.XP.Visible = true
		end
	end
	script.Parent.Parent.Parent.Disclaimer.YES.MouseButton1Click:Connect(function()
		disclaimerOption = "YES"
	end)
	script.Parent.Parent.Parent.Disclaimer.NO.MouseButton1Click:Connect(function()
		disclaimerOption = "NO"
	end)
	function saveCommand(command)
		if previouscommand1 ~= nil then
			if not alrset1 then
				if not alrset3 then
					print("1")
					alrset1 = true
					previouscommand1 = command
				else
					print("1")
					alrset2 = false
					alrset1 = true
					previouscommand1 = command
				end
			end
		else
			previouscommand1 = command
		end
		
		
		
		
		----
		if previouscommand2 ~= nil then
			if not alrset2 then
				if not alrset3 then
					print("2")
					alrset2 = true
					previouscommand2 = command
				else
					print("2")
					alrset2 = true
					alrset3 = false
					previouscommand2 = command
				end
			end
		else
			previouscommand1 = command
		end
		--
		if previouscommand3 ~= nil then
			if not alrset3 then
				print("3")
				alrset3 = true
				alrset1 = false
				previouscommand3 = command
			end
		else
			previouscommand1 = command
		end
	end
	function ownsGamepass(gamepassid)
		return game:GetService("MarketplaceService"):UserOwnsGamePassAsync(game.Players.LocalPlayer.UserId,gamepassid)
	end
	-- Cuadro de advertencia sobre comandos peligrosos.
	
	--------------------------------------------------------------
	--------------------------------------------------------------
	--------------------------------------------------------------
	--------------------------------------------------------------
	--------------------------------------------------------------
	--------------------------------------------------------------
	
	-- Código principal, core de la command bar.
	-- Variable "paused": Pausa el input mientras se procesa un comando largo.
	InputBox.FocusLost:Connect(
		function(enterPressed)
			if enterPressed then
				saveCommand(script.Parent.Text)
				if not paused then
					local command = script.Parent.Text
					local split_message = command:split(" ")
					local arg1 = string.lower(split_message[1])
					local arg2 = string.lower(split_message[2])
					local arg3 = string.lower(split_message[3])
					for i = 4,#split_message do
						arg3 = string.lower(arg3.." "..split_message[i])
					end
					if arg1 == "tp" or arg1 == "to" then -- Comando #1
						if arg2 then
							local player = GetPlayerFromString(arg2)
							if player then
								if player == game.Players.LocalPlayer then
									notifyerror("You cant teleport to yourself!")
								else
									if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Health > 0 then
										if player.Character:FindFirstChild("HumanoidRootPart") ~= nil then
											notifyok("Teleported to".." "..player.Name..".")
											game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(player.Character:FindFirstChild("HumanoidRootPart").Position)
										else
											notifyerror("The target is dead!")
										end
									else
										notifyerror("You are dead!")
									end
								end
							else
								notifyerror("Cannot find the player.")
							end
						else
							notifyerror("Specify a player!")
						end
					elseif arg1 == "cmds" or arg1 == "cmdlist" or arg1 == "cmd" then -- Comando #2
						if script.Parent.Parent.Parent.CMDS.Visible == true then
							script.Parent.Parent.Parent.CMDS.Visible = false
							notifyok("Command list has been closed.")
						else
							script.Parent.Parent.Parent.CMDS.Visible = true
							notifyok("Command list has been opened.")
						end
					elseif arg1 == "userinfo" then -- Comando #3
						notifyok("You're logged in as "..playerdiscordid)
					elseif arg1 == "output" then -- Comando #4
						if script.Parent.Parent.Parent.Output.Visible == true then
							script.Parent.Parent.Parent.Output.Visible = false
							notifyok("Output has been closed.")
						else
							script.Parent.Parent.Parent.Output.Visible = true
							notifyok("Output has been opened.")
						end
					elseif arg1 == "re" or arg1 == "respawn" then -- Comando #5
						paused = true
						notifyok("Respawned player.")
						local ogpos = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position
						wait()
						game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Health = 0
						game.Players.LocalPlayer.CharacterAdded:Wait()
						wait(1)
						game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(ogpos)
						paused = false
					elseif arg1 == "terminate" or arg1 == "term" then -- Comando #6
						if arg2 then
							local target = GetPlayerFromString(arg2)
							if target then
								if target.Team.Name == "Class-D" then
									paused = true
									notifyok("Making "..target.Name.." terminate!")
									team("Class-D")
									wait(3)
									terminate(target)
									paused = false
								else
									notifyerror("The player must be in Class-D team.")
								end
							else
								notifyerror("Cannot find the player.")
							end
						else
							notifyerror("Specify a player!")
						end
					elseif arg1 == "bring" then -- Comando #7
						if arg2 then
							local target = GetPlayerFromString(arg2)
							if target then
								if game.Players.LocalPlayer.Team.Name == "Foundation Personnel" or game.Players.LocalPlayer.Team.Name == "Scientific Department" then
									paused = true
									local ogpos2 = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position
									local ogteam = game.Players.LocalPlayer.Team.Name
									wait()
									team("Class-D")
									wait(1)
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(ogpos2)
									wait(1)
									notifyok("Bringed "..target.Name..".")
									bring(game.Players.LocalPlayer,target)
									wait(1)
									team(ogteam)
									wait(1)
									game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(ogpos2)
									paused = false
								else
									paused = true
									notifyok("Bringed "..target.Name..".")
									bring(game.Players.LocalPlayer,target)
									paused = false
								end
							else
								notifyerror("Cannot find the player.")
							end
						else
							notifyerror("Specify a player!")
						end
					elseif arg1 == "killall" or arg1 == "ka" then
						if getAmountOfPlayersInTeam("Chaos Insurgency") > 6 then
							notifyerror("CI Team is full! Try again later.")
						else
							paused = true
							local option = disclaimer()
							if option == true then
								local player = game.Players.LocalPlayer
								local ogpos = player.Character:FindFirstChild("HumanoidRootPart").Position
								local ogteam = player.Team.Name
								local function closeCIUniformGUI()
									for i = 0, 10 do
										wait()
										game.Players.LocalPlayer.PlayerGui.Uniform.Enabled = false
									end
								end
								local function citeam()
									game:GetService("ReplicatedStorage").RemoteFunction.RequestTeam:InvokeServer("Chaos Insurgency")
								end
								local function getWeapon()
									if player.Backpack:FindFirstChild("Makarov") ~= nil then
										return "Makarov"
									elseif player.Backpack:FindFirstChild("AKM") ~= nil then
										return "AKM"
									elseif player.Backpack:FindFirstChild("Skorpion VZ-6") ~= nil then
										return "Skorpion VZ-6"
									elseif player.Backpack:FindFirstChild("RPK") ~= nil then
										return "RPK"
									else
										return false
									end
								end
								notifyok("Killing everyone!")
								FakeChar(true)
								spoofPlayerInfo(true)
								citeam()
								repeat
									wait()
								until player.Team.Name == "Chaos Insurgency"
								closeCIUniformGUI()
								for i=0,10 do
									wait(0.2)
									hasWeapon = getWeapon()
								end
								closeCIUniformGUI()
								if hasWeapon then
									closeCIUniformGUI()
									local humanoid = player.Character:FindFirstChild("Humanoid")
									if humanoid then
										closeCIUniformGUI()
										humanoid:EquipTool(player.Backpack[hasWeapon])
										wait(1)
										for i,v in pairs(game.Players:GetChildren()) do
											closeCIUniformGUI()
											if v and v.Team ~= nil then
												if v.Team.Name == "Class-D" then
													print(v.Name.." is a Class D. Cancelling kill.")
												elseif v.Team.Name == "SCP610" then
													cikill(v, hasWeapon, 500)
												elseif v.Team.Name == "SCP457" then
													cikill(v,hasWeapon,2000)
												else
													cikill(v,hasWeapon,5)
												end
											end
										end
										notifyok("Killed everyone but CDs and CIs.")
									end
								else
									notifyerror("Failed to equip weapon.")
								end
								wait(1)
								team(ogteam)
								wait(1)
								player.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(ogpos)
								FakeChar(false)
							else
								notifyerror("Cancelled command execution.")
							end
							paused = false
							spoofPlayerInfo(false)
						end
					elseif arg1 == "ws" or arg1 == "walkspeed" then
						if arg2 then
							if typeof(tonumber(arg2)) == "number" then
								game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = arg2
								notifyok("Set walk speed to".." "..tonumber(arg2)..".")
							else
								notifyerror("Specify a number, not letters!")
							end
						else
							notifyerror("Specify a number!")
						end
					elseif arg1 == "rgens" or arg1 == "repairgens" then
						if fireproximityprompt then
							notifyok("Repairing generators.")
							paused = true
							local ogteam = game.Players.LocalPlayer.Team.Name
							local ogpos = game.Players.LocalPlayer.Character["HumanoidRootPart"].Position
							wait()
							local args = {
								[1] = "Engineering & Technical"
							}
							game:GetService("ReplicatedStorage").RemoteFunction.RequestTeam:InvokeServer(unpack(args))
							wait(1)
							game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack["Hammer"])
							for i,v in pairs(game.Workspace.Area02.Generators:GetDescendants()) do
								if v:IsA("ProximityPrompt") then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Parent.Position)
									wait(1)
									fireproximityprompt(v)
									wait()
									fireproximityprompt(v)
									wait()
									fireproximityprompt(v)
									wait()
									fireproximityprompt(v)
									wait()
									fireproximityprompt(v)
									wait()
									fireproximityprompt(v)
									fireproximityprompt(v)
									fireproximityprompt(v)
									fireproximityprompt(v)
									fireproximityprompt(v)
								end
							end
							team(ogteam)
							wait(3)
							game.Players.LocalPlayer.Character["HumanoidRootPart"].CFrame = CFrame.new(ogpos)
							paused = false
						else
							notifyerror("Unsupported exploit.")
						end
					elseif arg1 == "jp" or arg1 == "jumppower" then
						if arg2 then
							if typeof(tonumber(arg2)) == "number" then
								game.Players.LocalPlayer.Character.Humanoid.JumpPower = arg2
								notifyok("Set jump power to".." "..tonumber(arg2)..".")
							else
								notifyerror("Specify a number, not letters!")
							end
						else
							notifyerror("Specify a number!")
						end
					elseif arg1 == "hh" or arg1 == "hipheight" then
						if arg2 then
							if typeof(tonumber(arg2)) == "number" then
								game.Players.LocalPlayer.Character.Humanoid.HipHeight = arg2
								notifyok("Set hip height to".." "..tonumber(arg2)..".")
							else
								notifyerror("Specify a number, not letters!")
							end
						else
							notifyerror("Specify a number!")
						end
					elseif arg1 == "rhh" or arg1 == "resethipheight" then
						notifyok("Resetted your hip height.")
						game.Players.LocalPlayer.Character.Humanoid.HipHeight = 0
					elseif arg1 == "rjp" or arg1 == "resetjumppower" then
						notifyok("Resetted your jump power.")
						game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
					elseif arg1 == "rws" or arg1 == "resetwalkspeed" then
						notifyok("Resetted your walk speed.")
						game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
					elseif arg1 == "view" then
						if arg2 then
							local target = GetPlayerFromString(arg2)
							if target then
								if target.Character["Humanoid"] ~= nil then
									notifyok("Viewing "..target.Name..".")
									workspace.CurrentCamera.CameraSubject = target.Character
								else
									notifyerror("Target player is dead.")
								end
							else
								notifyerror("Cannot find player.")
							end
						else
							notifyerror("Specify a player!")
						end
					elseif arg1 == "unview" then
						notifyok("Returned camera to you.")
						workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
					elseif arg1 == "team" or arg1 == "t" then
						if arg2 == "scientificdepartment" or arg2 == "scd" or arg2 == "scientific" then
							team("Scientific Department")
						elseif arg2 == "securitydepartment" or arg2 == "sd" or arg2 == "security" then
							team("Security Department")
						elseif arg2 == "ci" or arg2 == "chaosinsurgency" or arg2 == "chaos" then
							team("Chaos Insurgency")
						elseif arg2 == "departmentofexternalrelations" or arg2 == "dea" or arg2 == "externalrelations" then
							team("Department Of External Relations")
						elseif arg2 == "et" or arg2 == "engineeringandtechnical" or arg2 == "engineering" or arg2 == "technical" then
							team("Engineering & Technical")
						elseif arg2 == "class-d" or arg2 == "classd" or arg2 == "cd" then
							team("Class-D")
						elseif arg2 == "ec" or arg2 == "ethicscommittee" or arg2 == "ethics" then
							team("Ethics Committee")
						elseif arg2 == "fp" or arg2 == "foundationpersonnel" or arg2 == "foundation" then
							team("Foundation Personnel")
						elseif arg2 == "internalsecuritydepartment" or arg2 == "isd" or arg2 == "internalsecurity" then
							local id = 5849461
							
						end
					else
						unknowncommand()
					end
				else
					notifyerror("Some command is being done. Wait!")
					notifyerror("If this is stuck, reset your character.")
				end
			end
		end
	)
	
	-- LOOPS --
	
	coroutine.wrap(function()
		while wait() do
			if FakeCameraSubject then
				spoofPlayerInfo(true)
				if game.Workspace["FakeChar"] ~= nil then
					game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false)
					game.Players.LocalPlayer.PlayerGui:WaitForChild("Uniform").Enabled = false
					game:GetService("Players").LocalPlayer.PlayerGui["Team Info UI"].Notification.Visible = false
					workspace.CurrentCamera.CameraSubject = game.Workspace["FakeChar"]
					game.Workspace["FakeChar"].Humanoid.Jump = true
				end
			else
				game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, true)
			end
		end
	end)()
end
coroutine.wrap(NFAU_fake_script)()
local function MOOATB_fake_script() -- TextButton.ShowOutput 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		if not shown then
			shown = true
			script.Parent.Parent.Parent.Parent.Output.Visible = true
		else
			shown = false
			script.Parent.Parent.Parent.Parent.Output.Visible = false
		end
	end)
end
coroutine.wrap(MOOATB_fake_script)()
local function BQNVG_fake_script() -- Button.ShowCMDS 
	local script = Instance.new('LocalScript', Button)

	script.Parent.MouseButton1Click:Connect(function()
		if not shown then
			shown = true
			script.Parent.Parent.Parent.Parent.CMDS.Visible = true
		else
			shown = false
			script.Parent.Parent.Parent.Parent.CMDS.Visible = false
		end
	end)
end
coroutine.wrap(BQNVG_fake_script)()
local function OIAC_fake_script() -- UIListLayout_2.AutoSize 
	local script = Instance.new('LocalScript', UIListLayout_2)

	script.Parent:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		local absoluteSize = script.Parent.AbsoluteContentSize
		script.Parent.Parent.CanvasSize = UDim2.new(0,absoluteSize.X, 0, absoluteSize.Y)
	end)
end
coroutine.wrap(OIAC_fake_script)()
local function OTNEQY_fake_script() -- ScrollingFrame_3.CommandAdder 
	local script = Instance.new('LocalScript', ScrollingFrame_3)

	-- Ver si el número es par.
	function Par(num)
		return num % 2 == 0
	end
	-- Lista de comandos.
	allcmds = {
		"-- Interface --",
		"cmds - Open/Close this tab.",
		"output - Open/Close output tab.",
		"userinfo - Get user information.",
		"--Player--",
		"team/t - Change your team. [SD/SCD/CI...]",
		"tp/to [PLR] - Teleport to a player.",
		"respawn/re - Respawn yourself.",
		"walkspeed/ws - Set your walkspeed.",
		"resetwalkspeed/rws - Reset your walkspeed.",
		"jumppower/jp - Set your jump power.",
		"resetjumppower/rjp - Reset your jump power.",
		"hipheight/hh - Set your hip height.",
		"resethipheight/rhh - Reset your hip height.",
		"fly/unfly - Turn fly mode on/off.",
		"view [PLR] - Spy a player.",
		"unview - Return camera to your humanoid.",
		"-- Server --",
		"term [PLR] - Terminate a CD. (Buggy!)",
		"bring [PLR] - Bring someone to you. (Buggy!)",
		"killall/ka - Kill everyone but CDs and CIs.",
		"rgens/repairgens - Repair light generators.",
		"",
	}
	wait(1)
	for i,v in pairs(allcmds) do
		local newtextbox = script.Parent.Parent.Parent.ExampleListLabel:Clone()
		newtextbox.Visible = true
		newtextbox.Text = v
		newtextbox.Parent = script.Parent.Parent.Parent.CMDS.ScrollingFrame
		if Par(i) then
			newtextbox.TextColor3 = Color3.fromRGB(255,255,255)
		else
			newtextbox.TextColor3 = Color3.fromRGB(173, 173, 173)
		end
	end
end
coroutine.wrap(OTNEQY_fake_script)()
